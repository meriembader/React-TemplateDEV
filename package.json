    "browserslist": "^4.17.2",
    "browserslist-config-google": "^3.0.1",
    "chalk": "^2.4.2",
    "compression": "1.7.4",
    "connected-react-router": "6.4.0",
    "cross-env": "^5.2.1",
    "eslint": "^7.32.0",
    "express": "4.16.4",
    "fontfaceobserver": "2.1.0",
    "google-map-react": "^2.1.10",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "immer": "3.0.0",
    "immutable": "^4.0.0-rc.15",
    "intl": "1.2.5",
