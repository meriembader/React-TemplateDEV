    "react-bootstrap-range-slider": "^1.2.2",
    "react-calendar": "^3.4.0",
    "react-dom": "16.8.6",
    "react-dropdown-select": "^4.7.4",
    "react-flags-select": "^1.1.13",
    "react-helmet": "6.0.0-beta",
    "react-icons": "^3.11.0",
    "react-intl": "2.8.0",
